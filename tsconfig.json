{
    "compilerOptions": {
        "target": "ES6",
        "alwaysStrict": true,
        "removeComments": true,
    },
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
}